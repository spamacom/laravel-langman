{
    "name": "spamacom/laravel-langman",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/spamacom/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Mouhammad Alali",
            "email": "mouhammad.alali@gmail.com",
            "homepage": "https://www.linkedin.com/in/jmalali/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "~5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "~5.1|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "~5.1|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.0",
        "orchestra/testbench": "^8.5.5",
        "mockery/mockery": "~1.5.1"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
